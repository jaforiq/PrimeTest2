@{
}

@model List<PrimeTest2.Models.Invoice>

<h1>All Purches</h1>

<table class=" table">
    <thead>
        <tr>
            <th>No</th>
            <th>Quentity</th>
            <th>Purches Date</th>
            <th>Total</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Quentity</td>
                <td>@item.PurchesDate</td>
                <td>@item.Total</td>
            </tr>
        }
    </tbody>

</table>